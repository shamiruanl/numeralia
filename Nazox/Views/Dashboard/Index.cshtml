        }

    /* Estilo para el contenido de las pestañas */
    .tab-content {
        display: none;
        padding: 20px;
        border: 1px solid #ddd;
        border-top: none;
    }

    /* Estilos adicionales para las cards */
    .card-title {
        font-size: 16px;
