    Layout = "~/Views/_Shared/_Layout.cshtml";
}

<!-- Incio CSS -->
<!-- select2 css -->
<link href="~/assets/libs/select2/css/select2.min.css" rel="stylesheet" type="text/css" />

<!-- Fin CSS -->

<style>
    input {
        height: 30px;
    }

    .form-control {
        height: 30px;
        padding: .2rem .75rem;
    }

    .btn {
        height: 30px;
        line-height: 1;
    }
</style>

<!-- Filtros -->
<div class="row">
    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 col-xxl-12">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Filtros</h4>
                <form>
                    <div class="row">
                        <div class="col-xl-3">
                            <div class="mb-3">
                                <label class="form-label" for="factura">Factura</label>
                                <input id="" name="factura" type="text" class="form-control">
                            </div>
                        </div>
                        <div class="col-xl-3">
                            <div class="mb-3">
                                <label class="form-label" for="cliente">Cliente</label>
                                <input id="" name="cliente" type="text" class="form-control">
                            </div>
                        </div>
                        <div class="col-xl-3">
                            <div class="mb-3">
                                <label class="form-label" for="rfc">RFC</label>
                                <input id="" name="rfc" type="text" class="form-control">
                            </div>
                        </div>
                        <div class="col-xl-3">
                            <div class="mb-3">
                                <label class="form-label control-label">Estatus</label>
                                <select class="form-control select2" style="">
                                    <option>--Seleccionar--</option>
                                    <option value="">Activo</option>
                                    <option value="">Inactivo</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-xl-3">
                            <div class="mb-3">
                                <label class="form-label control-label">Centro de Servicio</label>
                                <select class="form-control select2">
                                    <option>--Seleccionar--</option>
                                    <option value="">Servicio 1</option>
                                    <option value="">Servicio 1</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-xl-2">
                            <div class="" style="margin-top: 28px;">
                                <button type="button" class="btn btn-info waves-effect waves-light" style="">Buscar</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

